
@{
    ViewData["Title"] = "Product Category";
    var parentDivModel = new ModalDialogViewModel { ParentDivId = "divProductCategory" };
}
<environment names="Development">
    <link rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/sweetalert/dist/sweetalert.css" />
</environment>
<environment names="Staging,Production">
    <link rel="stylesheet" href="~/lib/datatables/css/dataTables.bootstrap4.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/sweetalert/dist/sweetalert.css" />
</environment>
<div class="box">
    <div class="box-body table-responsive">
        <table class="table table-bordered table-hover" id="productCategoryTableId" cellspacing="0" align="center">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Name</th>
                    <th>Description</th>
                    <th>IsActive</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
<partial name="ModalDialog" model="@parentDivModel" />
@section Scripts{
    <script src="~/js/ProductCategory/index.js"></script>
    <script>
        var baseUrl = "@Url.Content("~/")";
    </script>
}
